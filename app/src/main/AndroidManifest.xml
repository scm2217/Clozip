<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cruzhacks2018.clozip">

    <application
        android:name=".OutfitManager"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ClozipMenu"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChooseOutfit" />
        <activity android:name=".ExistingOutfits" />
        <activity android:name=".ClothingInformation" />
        <activity android:name=".OutfitList" />
        <activity android:name=".PantsList" />
        <activity android:name=".ShirtList" />
        <activity android:name=".CoatList"></activity>
    </application>

</manifest>